try:
    while True:
    
        a= input()
        a_1=0;a_2=0;a_3=0;a_4=0
        for i in a:
            if i>='a' and i<='z':
                a_1+=1
            elif i>='A' and i<='Z':
                a_2+=1
            elif i>='0' and i<='9':
                a_3+=1
            elif i ==' ':
                a_4+=1
        print(a_1,end=(' '))
        print(a_2,end=(' '))
        print(a_3,end=(' '))
        print(a_4,end=(' '))
except:
    exit()


        elif a_1==20:
            k=['K']+k
        elif a_1==21:
            k=['L']+k
        elif a_1==22:
            k=['M']+k
        elif a_1==23:
            k=['N']+k
        elif a_1==24:
            k=['O']+k
        elif a_1==25:
            k=['P']+k
        elif a_1==26:
            k=['Q']+k
        elif a_1==27:
            k=['R']+k
        elif a_1==28:
            k=['S']+k
        elif a_1==29:
            k=['T']+k
        elif a_1==30:
            k=['U']+k
        elif a_1==31:
            k=['V']+k
        elif a_1==32:
            k=['W']+k
        elif a_1==33:
            k=['X']+k
        elif a_1==34:
            k=['Y']+k
        elif a_1==35:
            k=['Z']+k
    else:
        k=[str(a_1)]+k
        
        

a_1=n
        
if n==0:
    pass
else:
    if a_1==10:
        k=['A']+k
    elif a_1==11:
        k=['B']+k
    elif a_1==12:
        k=['C']+k
    elif a_1==13:

n,b=map(int,input().split())

k=[]
while n//b !=0:
    a_1=0
    a_1=n%b
    
    n=n//b
    
    if a_1>=10:
        if a_1==10:
            k=['A']+k
        elif a_1==11:
            k=['B']+k
        elif a_1==12:
            k=['C']+k
        elif a_1==13:
            k=['D']+k
        elif a_1==14:
            k=['E']+k
        elif a_1==15:
            k=['F']+k
        elif a_1==16:
            k=['G']+k
        elif a_1==17:
            k=['H']+k
        elif a_1==18:
            k=['I']+k
        elif a_1==19:

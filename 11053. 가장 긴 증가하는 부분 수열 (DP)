#if l_1[n]>l_1[n-1],l_1[n-2] ****  l_1[1]  -->   k.append() ->     min(dp[1] ******  dp[n-1])+1
n=int(input())
l_1=list(map(int,input().split()))
dp=[1]

for i in range(1,n):
    k=[]
    for j in range(i):
        if l_1[j]<l_1[i]:
            k.append(dp[j])
            
    if k:
        dp.append(max(k)+1)
    else:
        dp.append(1)
    
print(max(dp))


n= int(input())
l_1=list(map(int,input().split()))
dp=[1]
for i in range(1,n):
    k=[]
    for j in range(i):
        if l_1[i]<l_1[j]:
            k.append(dp[j])
        
    if k:
        dp.append(max(k)+1)
    else:
        dp.append(1)
        
print(max(dp))


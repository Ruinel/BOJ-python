n=int(input())
if n==1:
    j=int(input())
    
else:
    m=list(map(int,input().split()))
    i_1=0
    j=m[0]
    for i in m:
        i_1+=i
        
        if i_1>j:
             j=i_1
        if i_1<0:
             i_1=0


        
print(j)
        
        
     #문제 https://www.acmicpc.net/submit/1912
